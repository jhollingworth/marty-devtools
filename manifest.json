{
  "name": "Marty Developer Tools",
  "version": "0.0.1",
  "description": "Chrome Developer Tools for marty.js.",
  "devtools_page": "devtools.html",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "web_accessible_resources": [ "devtools.html", "devtools.js", "app/index.html", "dist/marty-devtools.js" ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["inject.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "minimum_chrome_version": "21.0.1180.57"
}